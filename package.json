{
  "name": "trace",
  "author": "Adrian C. Miranda",
  "description": "Displays expressions, or writes to log files, while debugging.",
  "version": "1.0.10",
  "licenses": "MIT",
  "main": "index.js",
  "browser": "index.js",
  "module": "index",
  "homepage": "https://github.com/adriancmiranda/trace",
  "readmeFilename": "README.md",
  "license": "MIT",
  "devDependencies": {
    "chai": "~3.5.0",
    "conventional-commit-types": "~2.1.0",
    "eslint": "~3.19.0",
    "eslint-config-airbnb-base": "~11.1.3",
    "eslint-plugin-import": "~2.2.0",
    "husky": "~0.13.3",
    "mocha": "~3.3.0",
    "npm-run-all": "~4.0.2",
    "should": "~11.2.1",
    "sinon": "~2.1.0",
    "sinon-chai": "~2.9.0",
    "standard-version": "~4.0.0",
    "uglify-js": "~2.8.22"
  },
  "scripts": {
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "start": "npm cache clean && npm i && npm run watch",
    "prebuild": "run-s eslint test",
    "lint": "eslint ./ --ignore-pattern \"trace.min.js\" --ignore-pattern \"**/node_modules/**\"",
    "test": "mocha -R spec test",
    "test:watch": "run-s \"test -- -w\"",
    "test:travis": "npm test",
    "test:appveyor": "npm test",
    "build": "uglifyjs -c -v trace.js -o trace.min.js",
    "precommit": "run-s test",
    "release": "standard-version"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adriancmiranda/trace.git"
  },
  "bugs": {
    "url": "https://github.com/adriancmiranda/trace/issues"
  },
  "keywords": [
    "trace",
    "console",
    "javascript",
    "browser",
    "library"
  ],
  "greenkeeper": {
    "ignore": [
      "material"
    ]
  }
}
